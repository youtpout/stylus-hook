{
	"compilerOptions": {
		"target": "es2018",
		"module": "commonjs",
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"lib": [ "es2018" ],
		"typeRoots": [ "./node_modules/@types", "./scripts/typings" ],
		"rootDir": "scripts",
	}
}
